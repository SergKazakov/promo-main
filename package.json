{
  "private": true,
  "name": "promo-main",
  "homepage": "http://embermvp.com",
  "twitter": "",
  "repository": "",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:css": "stylefmt -R web_modules && stylelint \"web_modules/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "start": "phenomic start",
    "build": "phenomic build",
    "pretest": "npm run lint",
    "test": "npm run build",
    "precommit": "npm run pretest",
    "prepush": "npm run pretest"
  },
  "contactUrl": "https://docs.google.com/forms/d/1PMAvWDgx8buxlQyHPjP7dRwsCI7fnWbFGovquu9pzCs/formResponse",
  "phenomic": {
    "assets": false,
    "CNAME": true,
    "open": false,
    "devPort": 3000
  },
  "babel": {
    "presets": [
      "babel-preset-react",
      "babel-preset-es2015",
      "babel-preset-stage-1"
    ],
    "env": {
      "production": {
        "presets": [
          "babel-preset-react-optimize"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint-config-i-am-meticulous/react"
    ],
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/prop-types": [
        1,
        {
          "ignore": [
            "children",
            "className"
          ]
        }
      ],
      "react/jsx-no-bind": [
        1,
        {
          "ignoreRefs": true
        }
      ]
    },
    "globals": {
      "React": false,
      "ReactDOM": false,
      "Helmet": false
    }
  },
  "eslintConfigRuleReact/prefer-stateless-function": "https://github.com/MoOx/phenomic/issues/46",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.0.0-beta.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.13.0",
    "css-loader": "^0.23.0",
    "css-mqpacker": "^5.0.1",
    "eslint": "^3.2.2",
    "eslint-config-i-am-meticulous": "^4.1.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "husky": "^0.11.6",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.2",
    "npm-run-all": "^2.3.0",
    "phenomic": "^0.15.0",
    "postcss-center": "^1.0.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-font-magician": "^1.4.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-sorting": "^1.6.1",
    "postcss-triangle": "^1.0.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "stylefmt": "^4.2.3",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.1"
  },
  "dependencies": {
    "animate.css": "^3.5.1",
    "classnames": "^2.2.5",
    "history": "^2.0.2",
    "muicss": "^0.6.9",
    "normalize.css": "^4.2.0",
    "radium": "^0.18.1",
    "react": "^15.2.1",
    "react-burger-menu": "^1.10.2",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.3.0",
    "react-scroll": "^1.2.0",
    "react-slick": "^0.12.3",
    "react-svg-inline": "^1.1.0",
    "react-toastr": "^2.8.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "redux": "^3.0.0",
    "slick-carousel": "^1.6.0",
    "whatwg-fetch": "^1.0.0"
  }
}
